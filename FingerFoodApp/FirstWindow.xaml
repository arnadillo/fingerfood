<Window x:Class="FingerFoodApp.FirstWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FingerFood" Height="655" Width="375" WindowStartupLocation="CenterScreen">

    <Grid>
        <Frame Name ="navFrame" HorizontalAlignment="Left" Height="624" VerticalAlignment="Top" Width="365" Navigated="Frame_Navigated" NavigationUIVisibility="Hidden"/>

        <Canvas x:Name="navHeader">
            <Button x:Name="Menu_Button" Content="Menu" HorizontalAlignment="Left" Height="27" Margin="286,33,0,0" VerticalAlignment="Top" Width="61" BorderBrush="Red" Foreground="White" Background="Red" Click="Menu_Clicked"/>
            <Button x:Name="Back_Button" Content="&lt; Back" HorizontalAlignment="Left" Height="27" Margin="10,33,0,0" VerticalAlignment="Top" Width="61" Background="Red" Foreground="White" BorderBrush="Red" Click="Back_Clicked"/>
        </Canvas>
    </Grid>
</Window>
